Algoritmo "10"
Var

   num: inteiro

Inicio
   escreval("Digite um número:")
   leia(num)
   se num < 10 entao
      escreval("O número é menor que 10")
   senao
      se num = 10 entao
         escreval("O número é igual a 10")
      senao
         escreval("O número é maior que 10")
      fimse
   fimse
fimalgoritmo
