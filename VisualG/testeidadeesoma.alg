algoritmo "idade-das-pessoas"

var
   idade, soma: inteiro
   contador: inteiro
inicio

      soma <- 0
      
      para contador de 1 ate 5 faca
      escreval("Digite a idade da pessoa")
      leia(idade)
      soma <- soma + idade
      fimpara
      
      escreval("A soma das idade é ", soma)

fimalgoritmo
